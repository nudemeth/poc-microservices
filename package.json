{
  "name": "poc-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel fake-json prod",
    "test": "cross-env NODE_ENV=test jest",
    "jest-update": "cross-env NODE_ENV=test jest --updateSnapshot",
    "prod": "cross-env NODE_ENV=production node server.js",
    "dev": "npm-run-all --parallel fake-json dev-server",
    "dev-server": "node server.js",
    "build": "next build",
    "export": "next export",
    "analyze": "cross-env ANALYZE=1 next build",
    "fake-json": "json-server --watch fake/api/db.json --routes fake/api/routes.json --port 5000"
  },
  "dependencies": {
    "cross-env": "^5.1.3",
    "express": "^4.16.2",
    "isomorphic-unfetch": "^2.0.0",
    "material-ui": "^1.0.0-beta.33",
    "next": "^5.0.0",
    "next-redux-saga": "^2.0.1",
    "next-redux-wrapper": "^1.3.5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "serve-favicon": "~2.4.5",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "jest": "^22.2.2",
    "json-server": "^0.12.1",
    "npm-run-all": "^4.1.2",
    "sinon": "^4.3.0",
    "webpack-bundle-analyzer": "^2.10.0"
  }
}
